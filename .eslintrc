{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "jQuery": true,
    "$": true,
    "_": false,
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "max-len": ["error", 80, {"ignoreRegExpLiterals": true, "ignoreStrings": true, "ignoreUrls": true}],
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "prefer-template": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "react/no-unused-prop-types": 0,
    "react/no-danger": 0,
    "jsx-a11y/label-has-for": 0
  }
}
